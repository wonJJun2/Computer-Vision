   "source": [
    "# Gaussian Blur\n",
    "\n",
    "import cv2\n",
    "image = cv2.imread('gray_image.jpg')\n",
    "plt.imshow(cv2.cvtColor(image, cv2.COLOR_BGR2RGB))\n",
    "plt.show()\n",
    "\n",
    "# kernel_size : 홀수\n",
    "dst = cv2.GaussianBlur(image, (5,5), 0)\n",
    "plt.imshow(cv2.cvtColor(dst, cv2.COLOR_BGR2RGB))\n",
    "plt.show()"
   ]
  }
